page {
  background: #f8f8f8; }

.topBox {
  position: relative;
  width: 100%;
  height: 100rpx;
  background: #fff; }
  .topBox .topBoxInner {
    position: relative;
    height: 60rpx;
    padding: 20rpx 20rpx 20rpx 220rpx; }
  .topBox .positionBox {
    position: absolute;
    left: 15rpx;
    top: calc(50% - 30rpx);
    color: #333;
    font-size: 26rpx;
    height: 60rpx; }
    .topBox .positionBox .positionInner {
      height: 100%;
      position: relative;
      padding-left: 50rpx;
      display: flex;
      align-items: center;
      justify-content: center; }
    .topBox .positionBox .positionFlex {
      flex: 1; }
    .topBox .positionBox .posPosition {
      position: absolute;
      top: 3rpx;
      left: 0;
      width: 36rpx;
      height: 48rpx;
      background: url("data:image/png;base64,") no-repeat center center;
      background-size: 100% 100%; }
      .topBox .positionBox .posPosition.current {
        background: url("data:image/png;base64,") no-repeat center center;
        background-size: 100% 100%; }
    .topBox .positionBox .ico_arrowRight {
      position: absolute;
      right: 10rpx;
      top: calc(50% - 15rpx);
      display: inline-block;
      width: 30rpx;
      height: 30rpx; }
    .topBox .positionBox .posText {
      height: 40rpx;
      line-height: 40rpx;
      width: 140rpx;
      text-overflow: ellipsis;
      overflow: hidden;
      white-space: nowrap;
      font-size: 30rpx;
      color: #000;
      font-weight: bold; }
    .topBox .positionBox .posDistance {
      height: 30rpx;
      line-height: 30rpx;
      font-size: 24rpx;
      color: #666; }
  .topBox .searchBox {
    position: relative;
    width: 100%;
    height: 60rpx;
    line-height: 60rpx; }
    .topBox .searchBox .searchInner {
      position: relative;
      height: 100%;
      padding-left: 60rpx;
      background: #f5f5f5;
      border-radius: 50rpx;
      -webkit-border-radius: 50rpx;
      -moz-border-radius: 50rpx;
      -ms-border-radius: 50rpx;
      -o-border-radius: 50rpx; }
    .topBox .searchBox.tap {
      background: rgba(255, 247, 231, 0.9); }
    .topBox .searchBox .search-icon {
      position: absolute;
      left: 20rpx;
      top: calc(50% - 17.5rpx);
      width: 35rpx;
      height: 35rpx; }
    .topBox .searchBox .search-inp {
      height: 100%; }
    .topBox .searchBox .search-plac {
      font-size: 28rpx;
      color: #999; }

.textBox {
  width: 100%;
  padding: 20rpx 0;
  min-height: 40rpx;
  line-height: 40rpx;
  border-top: 1rpx solid #efefef;
  background: #fff; }
  .textBox .textInner {
    padding: 0 20rpx;
    font-size: 26rpx;
    color: #fc407d; }

.addressItemBox {
  margin-top: 30rpx;
  width: 100%; }
  .addressItemBox .addressItemInner {
    padding: 0 20rpx; }
  .addressItemBox .itemTitle {
    color: #8c8c8c;
    font-size: 30rpx; }
  .addressItemBox .itemList {
    margin-top: 15rpx;
    width: 100%;
    background: #fff;
    border-radius: 20rpx;
    -webkit-border-radius: 20rpx;
    -moz-border-radius: 20rpx;
    -ms-border-radius: 20rpx;
    -o-border-radius: 20rpx; }
    .addressItemBox .itemList .itemListInner {
      padding: 10rpx 30rpx; }
    .addressItemBox .itemList .itemLis {
      height: 80rpx;
      line-height: 80rpx;
      border-top: 1px solid #efefef; }
      .addressItemBox .itemList .itemLis:first-child {
        border-top: 0 none; }
      .addressItemBox .itemList .itemLis.itemMore .itemLisInner_lt, .addressItemBox .itemList .itemLis.itemMore .itemLisInner_rt {
        color: #636363; }
      .addressItemBox .itemList .itemLis .itemLisInner {
        display: table;
        table-layout: fixed;
        width: 100%; }
        .addressItemBox .itemList .itemLis .itemLisInner_lt, .addressItemBox .itemList .itemLis .itemLisInner_rt {
          display: table-cell;
          position: relative;
          vertical-align: middle;
          color: #333;
          font-size: 30rpx; }
        .addressItemBox .itemList .itemLis .itemLisInner_lt {
          color: #333; }
        .addressItemBox .itemList .itemLis .itemLisInner_rt {
          width: 120rpx;
          text-align: right; }
          .addressItemBox .itemList .itemLis .itemLisInner_rt .red {
            color: #fc407d; }
          .addressItemBox .itemList .itemLis .itemLisInner_rt .ico_arrowRight {
            display: inline-block;
            width: 30rpx;
            height: 30rpx; }
